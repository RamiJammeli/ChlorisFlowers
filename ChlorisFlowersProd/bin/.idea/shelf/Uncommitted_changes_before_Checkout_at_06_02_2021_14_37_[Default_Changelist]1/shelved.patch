d>\r\n                                {% elseif item.typeLivraison==\"livraisonSurprise\" %}\r\n                                <td>Surprise</td>\r\n                                    {% endif %}\r\n\r\n\r\n\r\n                                <td>\r\n\r\n                                    {% if item.status ==\"En cours\" %} <a  class=\"btn btn-warning btn-icon-split\" >  <span style=\"color: white; width: 80px;height: 30px;\" class=\"-align-center\">{{ item.status }}</span>\r\n                                    </a> {% endif %}\r\n                                    {% if item.status ==\"Terminé\" %} <a  class=\"btn btn-success btn-icon-split\">  <span style=\"color: white ;width: 80px;height: 30px;\" class=\"-align-center\">{{ item.status }}</span>\r\n                                    </a> {% endif %}\r\n                                </td>\r\n                                <td class=\"pr-2\">\r\n\r\n                                    <a href=\"{{ path('detailsLivraison',{'idCmd': item.id }) }}\"><i class=\"fa fa-eye pr-2\" style=\"font-size:20px;color:#efa2a9\"></i></a>\r\n\r\n                                </td>\r\n\r\n\r\n                            </tr>\r\n                        {% endfor %}\r\n                        </tbody>\r\n                    </table>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    </div>\r\n\r\n    </div>\r\n    <!-- /.container-fluid -->\r\n\r\n\r\n\r\n\r\n{% endblock %}\r\n
